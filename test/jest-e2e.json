{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/./../src/$1",
      "^@auth/(.*)$": [
        "<rootDir>/./../src/auth/$1"
      ],
      "^@carts/(.*)$": [
        "<rootDir>/./../src/carts/$1"
      ],
      "^@common/(.*)$": [
        "<rootDir>/./../src/common/$1"
      ],
      "^@orders/(.*)$": [
        "<rootDir>/./../src/prisma/$1"
      ],
      "^@productLikes/(.*)$": [
        "<rootDir>/./../src/product-likes/$1"
      ],
      "^@products/(.*)$": [
        "<rootDir>/./../src/product/$1"
      ],
      "^@users/(.*)$": [
        "<rootDir>/./../src/users/$1"
      ],
      "^@mail/(.*)$": [
        "<rootDir>/./../src/mail/$1"
      ]
    }
}
